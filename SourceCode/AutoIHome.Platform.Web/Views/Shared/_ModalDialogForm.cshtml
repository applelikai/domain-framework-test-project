<!--modal-dialog下的modal-content里面的内容模板-->
<partial name="_ModalDialogHeader" />
<div class="modal-body">
    @base.RenderBody()
</div>
<div class="modal-footer">
    <primary-button class="me-2" click="$(this).parents('.modal-content').find('form').submit();">
        <i class="fa fa-save"></i>&nbsp;保&nbsp;存
    </primary-button>
    <secondary-button click="close_dialog($(this).parents('.modal').get(0));">
        取&nbsp;&nbsp;消
    </secondary-button>
</div>