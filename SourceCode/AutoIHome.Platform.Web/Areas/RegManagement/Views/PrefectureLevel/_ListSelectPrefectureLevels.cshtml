@model IPagedList<PrefectureLevel>
@{
    string functionName = base.ViewBag.Function;
    string selectFunction = base.ViewBag.SelectFunction;
}

<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>行政区代码</th>
            <th>省级行政区</th>
            <th>行政区名称</th>
            <th>行政区类型</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var prefectureLevel in base.Model)
        {
            <tr onclick="@($"{selectFunction}('{prefectureLevel.PrefectureCode}', '{prefectureLevel.PrefectureName}');")">
                <td>@prefectureLevel.PrefectureCode</td>
                <td>@prefectureLevel.ProvinceLevel.ProvinceName</td>
                <td>@prefectureLevel.PrefectureName</td>
                <td>@prefectureLevel.PrefectureType</td>
            </tr>
        }
    </tbody>
    <tfoot>
        <tr>
            <td colspan="4">
                @await Html.PartialAsync("_Pagination", base.Model.GetPagination(functionName))
            </td>
        </tr>
    </tfoot>
</table>